#color aliases
alias ls="ls --color=auto"
alias grep="grep --color=auto"
alias diff="diff --color=auto"

#ls aliases
alias la="ls -A"
alias l="ls -CF"
alias ll="ls -Alh"

#parameter aliases
alias which="which -a"
alias brew="brew -v"
alias nmap="nmap -T4"

#parameter aliases
alias nano="vim"
alias lg="lazygit"
alias search="ddgr"
alias trash="trash -v -F"

#thefuck aliases
eval $(thefuck --alias)
