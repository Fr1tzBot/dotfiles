alias ls="ls --color=auto"
alias grep="grep --color=auto"
alias diff="diff --color=auto"

alias la="ls -A"
alias l="ls -CF"
alias ll="ls -Alh"

alias which="which -a"
alias brew="brew -v"
alias port="port -v"
alias nmap="nmap -T4"

alias nano="vim"
alias lg="lazygit"
alias search="ddgr"

alias trash="trash -v -F"
eval $(thefuck --alias)
