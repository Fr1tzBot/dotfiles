alias brew="brew -v"
alias ls="gls --color=auto"
alias grep="ggrep --color=auto"
alias fgrep="gfgrep --color=auto"
alias egrep="gegrep --color=auto"
alias la="ls -A"
alias l="ls -CF"
alias ll="ls -AlF"
alias cat="lolcat"
alias python="/opt/homebrew/opt/python@3.9/libexec/bin/python"
