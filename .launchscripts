#!/bin/bash
has() { type -p "$1" >/dev/null; }
if has ubuntu-bug ; then
    distro="Ubuntu"
elif has brew ; then
    distro="Mac OS"
elif has termux-open ; then
    distro="Termux"
elif has pacman ; then
    distro="Arch"
else
    distro=$(uname -s)
fi
printf "Good day, welcome to $distro!\n"
neofetch

